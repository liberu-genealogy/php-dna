{
    "name": "liberu-genealogy/php-dna",
    "description": "DNA analysis toolkit for PHP 8.4+",
    "type": "library",
    "keywords": ["dna", "genotype", "genealogy", "bioinformatics"],
    "homepage": "http://github.com/liberu-genealogy/php-dna",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "league/csv": "^9.0",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/http-client": "^7.0",
        "ext-json": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.8",
        "rector/rector": "^1.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Dna\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnaTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
